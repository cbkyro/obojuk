{"components/connect":"{{#if social_media}}\n  <nav class=\"social-nav\">\n    <h2 class=\"footer-menu-header footer-social-header\">{{lang 'common.footer.social'}}</h2>\n    <ul class=\"social-nav-list\">\n      {{#each social_media}}\n        <li class=\"social-link\">\n          <a href=\"{{url}}\" class=\"{{display_name}}\" target=\"_blank\">\n            <svg class=\"icon-social\">\n              <use xlink:href=\"#icon-{{#if display_name '===' 'google+'}}google-plus{{else}}{{display_name}}{{/if}}\"></use>\n            </svg>\n          </a>\n        </li>\n      {{/each}}\n    </ul>\n  </nav>\n{{/if}}"}