{"components/cart/cart-preview-link":"<a class=\"cart-preview-trigger\" data-cart-preview-toggle data-cart-count=\"{{cart.quantity}}\" href=\"{{urls.cart}}\">\n  <svg class=\"icon-cart\">\n    <use xlink:href=\"#icon-cart\"></use>\n  </svg>\n  <span class=\"cart-preview-label\">{{lang 'cart.preview.label'}}</span>\n  <span class=\"cart-preview-quantity\" id=\"cart-preview-count\" data-cart-preview-count>{{cart.quantity}}</span>\n</a>\n\n<div class=\"cart-preview-container\" data-cart-preview-container>\n  <h2 class=\"cart-preview-title\">{{lang 'cart.preview.title'}}</h2>\n  <button class=\"cart-preview-close\" data-cart-preview-toggle>\n    <svg class=\"icon-cart-close\">\n      <use xlink:href=\"#icon-cart-close\"></use>\n    </svg>\n  </button>\n  <div class=\"cart-preview\" data-cart-preview>\n    {{> components/cart/cart-preview}}\n  </div>\n</div>\n","components/cart/cart-preview":"\n{{#if cart.items}}\n  <div class=\"cart-preview-body {{#if cart.items.length '>' 2}}scroll{{/if}}\" data-count=\"{{cart.quantity}}\" data-cart-preview-body>\n\n    <ul class=\"cart-preview-list\">\n      {{#each cart.items}}\n        <li class=\"cart-preview-list-item\">\n          <div class=\"cart-preview-list-item-image\">\n            <a href=\"{{url}}\">\n              <img src=\"{{getImage image 'small' (cdn theme_settings.default_image)}}\" />\n            </a>\n          </div>\n\n          <div class=\"cart-preview-list-item-details\">\n            <div class=\"cart-preview-list-item-name\">\n              <a href=\"{{url}}\" title=\"{{name}}\">{{name}}</a>\n            </div>\n            {{#if options.length}}\n              <div class=\"cart-preview-list-item-options\">\n                {{#each options}}\n                  {{name}}:&nbsp;{{value}}{{#unless @last}}, {{/unless}}\n                {{/each}}\n              </div>\n            {{/if}}\n            <div class=\"cart-preview-list-item-quantity\">\n              {{lang 'cart.preview.quantity'}} {{quantity}}\n            </div>\n            <div class=\"cart-preview-list-item-remove\">\n              <a class=\"cart-preview-list-item-remove\" data-product-id=\"{{id}}\" href=\"{{remove_url}}\" data-cart-remove>{{lang 'cart.preview.remove'}}</a>\n             </div>\n          </div>\n\n          <div class=\"cart-preview-list-item-price\">\n            <span class=\"{{#if price_discounted}}price-discounted{{/if}}\">{{price.formatted}}</span>\n            {{#if price_discounted}}\n              <span class=\"cart-item-discount\">{{price_discounted.formatted}}</span>\n            {{/if}}\n          </div>\n        </li>\n      {{/each}}\n    </ul>\n\n  </div>\n  <div class=\"cart-preview-footer\">\n\n    {{#if cart.discount.value \"<\" 0}}\n      <div class=\"cart-preview-discount\">\n        <div class=\"cart-preview-total-price-label\">\n          {{lang 'cart.discount'}}\n        </div>\n        <div class=\"cart-preview-total-price\">\n          {{cart.discount.formatted}}\n        </div>\n      </div>\n    {{/if}}\n\n    <div class=\"cart-preview-pricing\">\n      <div class=\"cart-preview-total-price-label\">{{lang 'cart.preview.total'}}</div>\n      <div class=\"cart-preview-total-price\">&nbsp; {{{cart.sub_total.formatted}}}</div>\n    </div>\n    <a href=\"{{urls.cart}}\" class=\"button button-tertiary\"><span>{{lang 'cart.preview.view_cart'}}</span></a>\n    <a href=\"{{urls.checkout.single_address}}\" class=\"button button-primary\">{{lang 'cart.preview.checkout.button'}}</a>\n  </div>\n\n{{else}}\n  <p class=\"cart-preview-empty\">{{lang 'cart.preview.checkout.empty_cart'}}</p>\n{{/if}}\n<div id=\"cart-preview-overlay\" class=\"cart-preview-overlay\"></div>\n"}