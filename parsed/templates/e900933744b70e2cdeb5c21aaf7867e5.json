{"components/search/sidebar":"<div class=\"faceted-search\" data-faceted-search>\n  {{#if product_results.faceted_search_enabled}}\n    {{#each product_results.facets}}\n      {{#if total_items \"!=\" 0}}\n        {{#if visible}}\n          <div class=\"facet-filter\" data-facet-filter>\n\n            <h3 class=\"facet-section-heading\">\n              {{title}}\n\n              {{#if remove_url}}\n                <a class=\"facet-clear\" href=\"{{remove_url}}\" data-faceted-search-facet data-facet-remove>{{lang 'faceted_search.clear'}}</a>\n              {{/if}}\n\n              {{#unless selected }}\n                <span class=\"facet-toggle {{#if start_collapsed}}is-open{{/if}}\" data-facet-toggle>\n                  <svg class=\"icon-close-small\">\n                    <use xlink:href=\"#icon-close-small\"></use>\n                  </svg>\n                </span>\n              {{/unless}}\n            </h3>\n\n            {{{dynamicComponent 'components/faceted-search/facets'}}}\n          </div>\n        {{/if}}\n      {{/if}}\n    {{/each}}\n  {{/if}}\n</div>\n{{> components/common/sort sort=pagination.product_results.sort}}\n\n{{#if product_results.faceted_search_enabled}}\n  <a class=\"facet-clear-all\" href=\"{{category.selected.remove_all_url}}\" data-faceted-search-facet data-facet-remove>\n    {{lang 'faceted_search.clear_all'}}\n  </a>\n{{/if}}\n","components/common/sort":"<form class=\"sort-by\" method=\"get\" data-sort-by>\n  <div class=\"form-field form-field-product-sort form-field-select\">\n    <label class=\"form-label\">\n      {{! Todo: do we need the label? <span class=\"form-field-title\">\n        lang 'sorter.sort_by'\n      </span>}}\n      <span class=\"form-field-control\">\n        <span class=\"form-select-wrapper\" data-selected-prefix=\"{{lang 'sorter.sort_by'}}\">\n          <select class=\"form-input form-select\" name=\"sort\">\n            <option value=\"featured\" {{#compare sort 'featured' operator='==='}}selected{{/compare}}>{{lang 'sorter.featured'}}</option>\n            <option value=\"newest\" {{#compare sort 'newest' operator='==='}}selected{{/compare}}>{{lang 'sorter.newest'}}</option>\n            <option value=\"bestselling\" {{#compare sort 'bestselling' operator='==='}}selected{{/compare}}>{{lang 'sorter.top_sellers'}}</option>\n            <option value=\"alphaasc\" {{#compare sort 'alphaasc' operator='==='}}selected{{/compare}}>{{lang 'sorter.alpha_asc'}}</option>\n            <option value=\"alphadesc\" {{#compare sort 'alphadesc' operator='==='}}selected{{/compare}}>{{lang 'sorter.alpha_desc'}}</option>\n            <option value=\"avgcustomerreview\" {{#compare sort 'avgcustomerreview' operator='==='}}selected{{/compare}}>{{lang 'sorter.reviews'}}</option>\n            <option value=\"priceasc\" {{#compare sort 'priceasc' operator='==='}}selected{{/compare}}>{{lang 'sorter.price_asc'}}</option>\n            <option value=\"pricedesc\" {{#compare sort 'pricedesc' operator='==='}}selected{{/compare}}>{{lang 'sorter.price_desc'}}</option>\n          </select>\n        </span>\n      </span>\n    </label>\n  </div>\n</form>\n","components/faceted-search/facets/hierarchy":"<div data-facet-filter-wrapper class=\"facet-filter-wrapper {{#unless start_collapsed}}is-open{{/unless}}\">\n  <ul id=\"{{#replace ' ' facet}}{{else}}{{facet}}{{/replace}}\" class=\"facet-option {{#if selected}}is-open{{/if}} {{#unless start_collapsed}}is-open{{/unless}}\" data-count=\"{{items_count}}\">\n    {{#each items}}\n      <li class=\"{{#if selected}}facet-selected{{/if}}\" data-facet-item>\n        <a href=\"{{url}}\" rel=\"nofollow\" data-faceted-search-facet>\n          {{title}}\n          {{#if ../show_product_counts}}<span data-facet-item-count>({{count}})</span>{{/if}}\n        </a>\n      </li>\n    {{/each}}\n  </ul>\n\n  {{#if show_more_toggle}}\n    <a href=\"#faceted-search-navList-{{#replace ' ' facet}}{{else}}{{facet}}{{/replace}}\" role=\"button\"\n       class=\"toggleLink faceted-show-more-button {{#unless start_collapsed}}is-open{{/unless}}\"\n       data-show-more-facets>\n      <span class=\"toggle-link-text toggle-link-text-off\">{{lang 'faceted_search.show_more'}}</span>\n      <span class=\"toggle-link-text toggle-link-text-on\">{{lang 'faceted_search.show_less'}}</span>\n    </a>\n  {{/if}}\n</div>","components/faceted-search/facets/multi":"<div data-facet-filter-wrapper data-facet-filter-multi class=\"facet-filter-wrapper {{#unless start_collapsed}}is-open{{/unless}}\">\n  <ul id=\"faceted-search-nav-list-{{#replace ' ' facet}}{{else}}{{facet}}{{/replace}}\"\n      class=\"facet-option {{#if selected}}is-open{{/if}} {{#if items_count '<' total_items}}has-additional-filters{{/if}}\"\n      data-count=\"{{items_count}}\" data-facet=\"{{facet}}\">\n    {{#each items}}\n    <li class=\"{{#if selected}}facet-selected{{/if}}\" data-facet-item>\n      <a href=\"{{url}}\" rel=\"nofollow\" data-faceted-search-facet>\n        {{title}}\n        {{#if ../show_product_counts}}<span data-facet-item-count>({{count}})</span>{{/if}}\n      </a>\n    </li>\n    {{/each}}\n  </ul>\n\n  {{#if show_more_toggle}}\n    <a href=\"#faceted-search-nav-list-{{#replace ' ' facet}}{{else}}{{facet}}{{/replace}}\" role=\"button\"\n       class=\"toggleLink faceted-show-more-button {{#unless start_collapsed}}is-open{{/unless}}\"\n       data-show-more-facets>\n      <span class=\"toggle-link-text toggle-link-text-off\">{{lang 'faceted_search.show_more'}}</span>\n      <span class=\"toggle-link-text toggle-link-text-on\">{{lang 'faceted_search.show_less'}}</span>\n    </a>\n  {{/if}}\n</div>\n","components/faceted-search/facets/range":"<form class=\"facet-option form-range-facet {{#if selected}}is-open{{/if}} {{#unless start_collapsed}}is-open{{/unless}}\" method=\"get\" data-facet-filter-wrapper data-faceted-search-range>\n  {{#each current_selected_items}}\n    <input type=\"hidden\" name=\"{{param_name}}\" value=\"{{param_value}}\"/>\n  {{/each}}\n\n  <div class=\"form-field form-field-range\">\n    <div class=\"form-field-control field-min\">\n      <input\n        class=\"form-input\"\n        name=\"min_price\"\n        min=\"0\"\n        type=\"number\"\n        value=\"{{min_price}}\"\n        placeholder=\"{{lang 'faceted_search.min_price'}}\"\n        required\n      />\n    </div>\n\n    <div class=\"form-field-control field-max\">\n      <input\n        class=\"form-input\"\n        name=\"max_price\"\n        min=\"0\"\n        type=\"number\"\n        value=\"{{max_price}}\"\n        placeholder=\"{{lang 'faceted_search.max_price'}}\"\n        required\n      />\n    </div>\n\n    <div class=\"form-actions\">\n      <input class=\"button button-secondary\" type=\"submit\" value=\"{{lang 'faceted_search.update'}}\">\n    </div>\n  </div>\n</form>\n","components/faceted-search/facets/rating":"<ul class=\"facet-option {{#if selected}}is-open{{/if}} {{#unless start_collapsed}}is-open{{/unless}}\" data-facet-filter-wrapper>\n  {{#each items}}\n    {{#if count}}\n      <li class=\"facet-rating\" data-facet-item>\n        <a href=\"{{url}}\" rel=\"nofollow\" data-faceted-search-facet>\n          <span class=\"rating-label\">{{lang 'product.reviews.stars_count' NUM=param_value}}</span>\n          {{> components/product/product-rating rating=param_value}}\n          {{#if ../../show_product_counts}}<span class=\"rating-count\" data-facet-item-count>{{#if count}}{{count}}{{else}}0{{/if}}</span>{{/if}}\n        </a>\n      </li>\n    {{/if}}\n  {{/each}}\n</ul>\n","components/product/product-rating":"\n<div class=\"product-rating\">\n  <div class=\"rating-stars-readonly\" aria-hidden=\"true\">\n    {{#enumerate 1 5}}\n      {{#compare ../this.rating this operator='>='}}\n        <svg class=\"rating-star star-full\">\n          <use xlink:href=\"#icon-star\"></use>\n        </svg>\n      {{else}}\n        <svg class=\"rating-star star-empty\">\n          <use xlink:href=\"#icon-star-empty\"></use>\n        </svg>\n      {{/compare}}\n    {{/enumerate}}\n  </div>\n  <span class=\"rating-total-score visually-hidden\">{{this.rating}}</span>\n  {{#if this.total}}\n    <span class=\"rating-total-votes visually-hidden\">(<a href=\"#\">{{this.total}}</a>)</span>\n  {{/if}}\n</div>\n"}