{"components/faceted-search/facets/range":"<form class=\"facet-option form-range-facet {{#if selected}}is-open{{/if}} {{#unless start_collapsed}}is-open{{/unless}}\" method=\"get\" data-facet-filter-wrapper data-faceted-search-range>\n  {{#each current_selected_items}}\n    <input type=\"hidden\" name=\"{{param_name}}\" value=\"{{param_value}}\"/>\n  {{/each}}\n\n  <div class=\"form-field form-field-range\">\n    <div class=\"form-field-control field-min\">\n      <input\n        class=\"form-input\"\n        name=\"min_price\"\n        min=\"0\"\n        type=\"number\"\n        value=\"{{min_price}}\"\n        placeholder=\"{{lang 'faceted_search.min_price'}}\"\n        required\n      />\n    </div>\n\n    <div class=\"form-field-control field-max\">\n      <input\n        class=\"form-input\"\n        name=\"max_price\"\n        min=\"0\"\n        type=\"number\"\n        value=\"{{max_price}}\"\n        placeholder=\"{{lang 'faceted_search.max_price'}}\"\n        required\n      />\n    </div>\n\n    <div class=\"form-actions\">\n      <input class=\"button button-secondary\" type=\"submit\" value=\"{{lang 'faceted_search.update'}}\">\n    </div>\n  </div>\n</form>\n"}