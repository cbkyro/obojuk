{"components/slideshow":"\n<section class=\"slideshow slideshow-count-{{carousel.slides.length}} {{theme_settings.slideshow-aspect-ratio}}\" data-slideshow>\n  {{#each carousel.slides}}\n    <figure class=\"slideshow-item slideshow-item-{{@index}}\" data-slideshow-slide style=\"background-image: url('{{image}}');\">\n      <figcaption class=\"slideshow-item-info\">\n        {{#if heading}}\n          <h2 class=\"slideshow-item-heading\">{{heading}}</h2>\n        {{/if}}\n        {{#if text}}\n          <p class=\"slideshow-item-caption\">{{{text}}}</p>\n        {{/if}}\n        {{#if url}}\n          {{#if button_text}}\n            <span class=\"button slideshow-button\" href=\"{{{url}}}\">{{button_text}}</span>\n          {{/if}}\n        {{/if}}\n      </figcaption>\n      {{#if url}}\n        <a class=\"slideshow-item-link\" href=\"{{{url}}}\"></a>\n      {{/if}}\n    </figure>\n  {{/each}}\n</section>\n"}