{"components/common/newsletter-subscribe":"<form class=\"form subscription-form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n  <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n  <div class=\"form-field form-inline\">\n    <input class=\"hidden\" id=\"nl_first_name\" name=\"nl_first_name\" type=\"text\" value=\"bc\">\n    <label class=\"show-for-sr\" for=\"nl_email\">{{lang 'common.newsletter.email'}}</label>\n    <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.name}}\" placeholder=\"{{lang 'common.newsletter.heading'}}\">\n    <input type=\"hidden\" name=\"check\" value=\"1\">\n    <input class=\"button button-primary\" type=\"submit\" value=\"{{lang 'common.newsletter.submit'}}\">\n  </div>\n</form>\n"}